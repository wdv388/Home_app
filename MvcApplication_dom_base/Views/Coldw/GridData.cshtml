@model IEnumerable<MvcApplication_dom_base.Models.Coldwater>

@foreach (var item in Model) {
    <tr data-pkey="@item.ID">
        <td>
            @Html.DisplayFor(modelItem => item.Current)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Current1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CurrentTotal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Previous)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Difference)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Price)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.water_half)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Price_half)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.totaltopay_half)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.totaltoplayfull_half)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TotaltoPay)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.date)
        </td>
        <td class="rowControl hidden">
            <a href="#makeRowEditable" class="makeRowEditable">Edit</a> |
            <a href="#deleteRow" class="deleteRow">Delete</a>
        </td>
    </tr>
}
